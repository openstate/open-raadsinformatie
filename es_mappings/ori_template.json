{
  "index_patterns": ["ori_*", "osi_*", "ggm_*", "owi_*"],
  "settings": {
    "index": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    }
  },
  "mappings": {
    "dynamic_date_formats": ["yyyy-MM-dd'T'HH:mm:ss.SSSSSS", "strict_date_time", "strict_date_time_no_millis", "strict_date_optional_time"],
    "dynamic_templates": [
      {
        "disbled_context": {
          "match": "@context",
          "mapping": {
            "type": "object",
            "enabled": false
          }
        }
      },
      {
        "disabled_was_generated_by": {
          "match": "was_generated_by",
          "mapping": {
            "type": "object",
            "enabled": false
          }
        }
      },
      {
        "list_as_text": {
          "match": "@list",
          "mapping": {
            "type": "text"
          }
        }
      }
    ],
    "properties": {
      "@context": {
        "type": "object",
        "enabled": false
      },
      "@id": {
        "type": "keyword"
      },
      "@type": {
        "type": "keyword"
      },
      "biography": {
        "type": "text"
      },
      "chair": {
        "type": "text"
      },
      "content_type": {
        "type": "keyword"
      },
      "family_name": {
        "type": "text"
      },
      "image": {
        "type": "keyword"
      },
      "invitee": {
        "type": "text"
      },
      "is_referenced_by": {
        "type": "keyword"
      },
      "last_discussed_at" : {
        "type" : "date",
        "format" : "yyyy-MM-dd'T'HH:mm:ss.SSSSSS||strict_date_time||strict_date_time_no_millis||strict_date_optional_time"
      },
      "original_url": {
        "type": "keyword"
      },
      "parent": {
        "type": "keyword"
      },
      "phone": {
        "type": "text"
      },
      "text": {
        "type": "text"
      },
      "title": {
        "type": "text"
      },
      "name": {
        "type": "text"
      },
      "description": {
        "type": "text"
      },
      "neighborhood_polygons": {
        "type": "geo_shape"
      },
      "text_pages": {
        "type": "nested",
        "properties" : {
            "page_number" : {
              "type" : "long"
            },
            "text" : {
              "type" : "text"
            }
          }
      },
      "url": {
        "type": "keyword"
      }
    }
  }
}
